{
  "name": "@the0/sdk",
  "version": "1.0.0",
  "description": "the0 SDK for Node.js/TypeScript trading bots",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "the0",
    "trading",
    "bot",
    "algorithmic-trading",
    "sdk"
  ],
  "author": "the0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexanderwanyoike/the0",
    "directory": "sdk/nodejs"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
